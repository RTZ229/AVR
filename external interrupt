#define F_CPU 16000000
#include <avr/io.h>
#include <util/delay.h>
#include <avr/interrupt.h>
int counter;
double w,v;
int main(void)
{
	DDRD = 0;
	TCCR1A |=(1<<COM1A1);
	TCCR1B |=(1<<WGM12)|(1<<CS12)|(1<<CS10);
	TIMSK |= (1<<OCIE1A);
	OCR1A = 957;
	sei();
	 
}
ISR(INT0_vect)
{
	counter++
}
ISR(INT1_vect)
{
	counter++
}
ISR(TIMER1_COMPA_vect)
{
  w=counter*3.141592653589;
  v=w*0.3;
  
  counter = 0;
}
